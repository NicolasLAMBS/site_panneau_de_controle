<!-- app/Resources/views/base.html.twig -->
<!DOCTYPE html>
<html lang="fr-FR">
    <head>
        <meta http-equiv="Content-Type" content="text/html"; charset=utf-8" />
        <title>{% block title %}Panneau de controle{% endblock %} - Monkees</title>
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
        {% block stylesheets %}
            <link href="{{ asset('css/screen.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('js/jquery.min.js') }}" type="text/javascript" />
            <link href="{{ asset('js/bootstrap.min.js') }}" type="text/javascript" />

        {% endblock %}
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div id="encreTopIndex"></div>

        <section id="wrapper">
            <header id="header">
                <div class="top">
                    {% block navigation %}
                    {% endblock %}
                </div>
            </header>
        </section>
        <section>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12pub">
                        {% block body %}
                        {% endblock %}
                    </div>
                </div>
            </div>
        </section>
        <section>
            <div id="footer">
                {% block footer %}
                {% endblock %}
            </div>
        </section>
        {% block javascripts %}{% endblock %}
    </body>
</html>
