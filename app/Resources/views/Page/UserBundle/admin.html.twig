{# app/Resources/views/page/UserBundle/admin.html.twig #}
{% extends ":page/UserBundle:layout.html.twig" %}

{% block fos_admin_content %}
    <br />

    <h3>Formulaire d'ajout d'URL :</h3>

    {{ form_start(form, {'id': 'form_id'}) }}
        {{ form_errors(form) }}

        {{ form_row(form.url, {'id': 'form_url_id'}) }}
        {{ form_row(form.save, {'id': 'form_save_id'}) }}

    {{ form_end(form) }}

    <p>Et ici je peux voir la liste compl√®te des urls :</p>
    <br />

    {{ render(controller('UserBundle:listUrl:showUrl')) }}
{% endblock fos_admin_content %}

